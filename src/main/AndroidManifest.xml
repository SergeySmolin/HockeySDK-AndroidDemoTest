<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="net.hockeyapp.hockeysdksample">

    <application android:allowBackup="true" android:fullBackupContent="false" android:label="@string/app_name" android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="net.hockeyapp.android.appIdentifier"
            android:value="${HOCKEYAPP_APP_ID}" />

    </application>

</manifest>
